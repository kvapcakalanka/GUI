<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>net8.0-windows</TargetFramework>
    <Nullable>enable</Nullable>
    <ImplicitUsings>enable</ImplicitUsings>
    <UseWPF>true</UseWPF>
  </PropertyGroup>

  <ItemGroup>
    <None Remove="Data\data1.db" />
    <None Remove="Images\Bali.jpg" />
    <None Remove="Images\home.jpg" />
    <None Remove="Images\home1.jpg" />
    <None Remove="Images\New_York.jpg" />
    <None Remove="Images\Paris.jpg" />
  </ItemGroup>

  <ItemGroup>
    <Content Include="Data\data1.db">
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </Content>
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="System.Data.SQLite" Version="1.0.119" />
  </ItemGroup>

  <ItemGroup>
    <Resource Include="Images\Bali.jpg" />
  </ItemGroup>

  <ItemGroup>
    <Resource Include="Images\home1.jpg" />
    <Resource Include="Images\New_York.jpg" />
    <Resource Include="Images\Paris.jpg" />
